description="The GTK Engines package contains eight themes/engines and two additional engines for GTK2"
url="https://download.gnome.org/sources/gtk-engines"

contributors="Berlius, Greg"
packager="tnut <tnut@nutyx.org>"

makedepends=(gtk2 lua)

name=gtk-engines
version=2.20.2

source=(https://download.gnome.org/sources/gtk-engines/${version:0:4}/$name-$version.tar.bz2)
 
build() {

cd $name-$version
./configure --prefix=/usr 	
make
make DESTDIR=$PKG install
}
