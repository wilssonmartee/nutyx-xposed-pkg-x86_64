description="Python 3 Useful miscellaneous modules used by Logilab projects."
url="https://files.pythonhosted.org"

packager="spiky <spiky@nutyx.org>"
contributors="Pierre B., tnut"

makedepends=(python python-six)
run=(python)

name=python-astroid
version=2.9.2

source=(https://files.pythonhosted.org/packages/f4/2b/06f31f8e57cef68b2b0fe6c12c8d6972de12fbe843bb0e15f9d4d3a1c492/astroid-2.9.2.tar.gz)

build() {
    cd astroid-$version
    python3 setup.py check
    python3 setup.py install --prefix=/usr --root=$PKG
}
